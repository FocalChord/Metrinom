{
    "name": "client",
    "version": "0.1.0",
    "private": true,
    "jest": {
        "setupFilesAfterEnv": [
            "jest-enzyme"
        ],
        "testEnvironment": "enzyme",
        "collectCoverageFrom": [
            "src/**/*.{js,jsx}"
        ],
        "moduleNameMapper": {
            "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "tests/__mocks__/fileMock.js"
        }
    },
    "dependencies": {
        "@material-ui/core": "^4.9.13",
        "@material-ui/icons": "^4.9.1",
        "@material-ui/lab": "^4.0.0-alpha.52",
        "@nivo/pie": "^0.61.1",
        "@testing-library/jest-dom": "^4.2.4",
        "@testing-library/react": "^9.5.0",
        "@testing-library/user-event": "^7.2.1",
        "chart.js": "^2.9.3",
        "js-cookie": "^2.2.1",
        "material-auto-rotating-carousel": "^3.0.2",
        "moment": "^2.25.3",
        "pure-react-carousel": "^1.27.1",
        "react": "^16.13.1",
        "react-chartjs-2": "^2.9.0",
        "react-dom": "^16.13.1",
        "react-graph-vis": "^1.0.5",
        "react-loader-spinner": "^3.1.14",
        "react-router-dom": "^5.1.2",
        "react-router-last-location": "^2.0.1",
        "react-scripts": "3.4.1",
        "recharts": "^1.8.5",
        "string-similarity": "^4.0.1",
        "yargs-parser": "^18.1.3"
    },
    "scripts": {
        "start": "npm run watch:css && react-scripts start",
        "build": "npm run build:css && react-scripts build",
        "test": "jest",
        "eject": "react-scripts eject",
        "build:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
        "watch:css": "postcss src/styles/tailwind.css -o src/styles/main.css",
        "publish-client": "cd ../ && git subtree push --prefix client metrinom master"
    },
    "eslintConfig": {
        "extends": "react-app"
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "autoprefixer": "^9.7.4",
        "babel-eslint": "^10.1.0",
        "case-sensitive-paths-webpack-plugin": "^2.3.0",
        "dotenv": "^8.2.0",
        "enzyme": "^3.11.0",
        "enzyme-adapter-react-16": "^1.15.2",
        "eslint": "^6.8.0",
        "eslint-config-prettier": "^6.10.0",
        "eslint-config-react-app": "^5.2.1",
        "eslint-plugin-flowtype": "^4.0.0",
        "eslint-plugin-import": "^2.20.1",
        "eslint-plugin-jsx-a11y": "^6.2.3",
        "eslint-plugin-prettier": "^3.1.2",
        "eslint-plugin-react": "^7.19.0",
        "eslint-plugin-react-hooks": "^2.0.0",
        "husky": "^4.2.3",
        "jest-enzyme": "^7.1.2",
        "lint-staged": "^10.0.8",
        "postcss-cli": "^7.1.0",
        "prettier": "^2.0.1",
        "react-test-renderer": "^16.13.1",
        "tailwindcss": "^1.2.0"
    },
    "husky": {
        "hooks": {
            "pre-commit": "lint-staged"
        }
    },
    "lint-staged": {
        "*.js": "eslint --cache --fix",
        "*.{js,css,md}": "prettier --write"
    }
}
